<!-- Scaffolding generated by Casein v5.2.0.0 -->

<table class="table table-striped table-hover table-condensed">
  <thead>
    <tr>
    <% userid = Casein::AdminUser.find(current_user.id) %>
     <%= userid.name %>

      <th><%= casein_sort_link "Title", :title %></th>
      <th><%= casein_sort_link "Slug", :slug %></th>
      <th><%= casein_sort_link "Given url", :given_url %></th>
      
      <th>&nbsp;</th>
    </tr>
  </thead>

  <% links.each do |link| 
    if link.casein_admin_users_id == current_user.id
    title = link.title 
    display_slug = link.display_slug 
    created_at = link.created_at 
    given_url = link.given_url 
    slug = link.slug 
     %>

    <tr>
      <td><h5 class="text-warning"><%= casein_table_cell_link truncate(link.title, length: 30), casein_link_path(link) %></h5></td>
      <td><code><%= casein_table_cell_link link.slug, casein_link_path(link) %></code></td>
      <td><%= casein_table_cell_link truncate(link.given_url, length: 30), casein_link_path(link) %></td>
      
      </td>
    </tr>
    <% end %>
  <% end %>
</table>
